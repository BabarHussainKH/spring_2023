			/> <constructor-arg name="address"> <null/> </constructor-arg> <constructor-arg 
			name="langs"> <null/> </constructor-arg> -->
	</bean>

	<bean id="dev2" class="com.spring.core.beans.Developer">
		<!-- by property -->
